{
  "name": "stackgraph-mcp-server",
  "version": "0.1.0",
  "description": "MCP Server with Stackgraph tool",
  "type": "module",
  "scripts": {
    "dev": "tsc --noEmit && node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "start": "node build/index.js",
    "gen-redoc": "tsc --noEmit && tsx docs/gen-redoc.ts",
    "copydir": "node docs/copydir/index.js",
    "build-app1": "tsc --noEmit && tsc --target esnext --module commonjs --outDir build/app app/app.ts",
    "build-app": "node build-app.js"
  },
  "bin": {
    "cursor-tools": "./build/index.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.37.0",
    "@microsoft/tiktokenizer": "^1.0.9",
    "@modelcontextprotocol/sdk": "^1.4.1",
    "@octokit/rest": "^21.1.1",
    "archy": "^1.0.0",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "fastembed": "^1.14.1",
    "git-url-parse": "^16.0.1",
    "neo4j-driver": "^5.28.1",
    "uuid": "^11.1.0",
    "zod": "3.24.1",
    "zod-to-json-schema": "3.24.1"
  },
  "devDependencies": {
    "@types/archy": "^0.0.36",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/git-url-parse": "^9.0.3",
    "@types/node": "^22.14.1",
    "@types/xmldom": "^0.1.34",
    "clipboardy": "^4.0.0",
    "esbuild": "^0.25.2",
    "htm": "3.1.1",
    "preact": "10.26.5",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3"
  },
  "scarfSettings": {
    "enabled": false
  }
}
